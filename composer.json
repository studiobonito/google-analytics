{
    "name": "studiobonito/silverstripe-google-analytics",
    "description": "Provide basic Google Analytics integration for SilverStripe CMS.",
    "type": "silverstripe-module",
    "keywords": [
        "studiobonito",
        "silverstripe-google-analytics",
        "silverstripe",
        "google",
        "analytics"
    ],
    "homepage": "https://github.com/studiobonito/silverstripe-google-analytics",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Tom Densham",
            "email": "tom.densham@studiobonito.co.uk",
            "homepage": "http://www.studiobonito.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "silverstripe/framework": ">=3.1.2,<4.0",
        "silverstripe/cms": ">=3.1.2,<4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "symfony/class-loader": "~2.3",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
